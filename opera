			 eventMouseover: function(calEvent, jsEvent) {
			    var tooltip = '<div class="tooltipevent" style="width:250px;height:auto; border-radius: 10px; padding: 10px;background:#990000; color: #fff; position:absolute;z-index:10001;">' + calEvent.description + '</div>';
			    jQuery("body").append(tooltip);
			    jQuery(this).mouseover(function(e) {
			        jQuery(this).css('z-index', 10000);
			        jQuery('.tooltipevent').fadeIn('1000');
			        jQuery('.tooltipevent').fadeTo('10', 1.9);
			    }).mousemove(function(e) {
			        jQuery('.tooltipevent').css('top', e.pageY + 10);
			        jQuery('.tooltipevent').css('left', e.pageX + 20);
			    });
			},
			
			eventMouseout: function(calEvent, jsEvent) {
				
					    jQuery(this).css('z-index', 8);
					    jQuery('.tooltipevent').remove();
					
			},
		});
		
	});

